#!zsh

(( ! $# )) && set -- -ilc :
# SampShell_DISABLED=1 tracezsh

clenv \
	-i \
	-v SampShell_PROFILE= \
	-v SampShell_XTRACE= \
	-v SampShell_ROOTDIR \
	${SampShell_DISABLED+-vSampShell_DISABLED=1} \
	-v SampShell_CACHEDIR \
	-v SampShell_disable_highlight=$SampShell_disable_highlight \
	-v SHLVL \
	-v PS4='+$(typeset -Fg SECONDS; echo "$SECONDS"):%x:%I> ' \
	-- \
	=zsh \
	--xtrace \
	--sourcetrace \
	--prompt-subst \
	${==@}
