	"\x1B[7m\\xF7\x1B[27m", "\x1B[7m\\xF8\x1B[27m", "\x1B[7m\\xF9\x1B[27m", "\x1B[7m\\xFA\x1B[27m",
	"\x1B[7m\\xFB\x1B[27m", "\x1B[7m\\xFC\x1B[27m", "\x1B[7m\\xFD\x1B[27m", "\x1B[7m\\xFE\x1B[27m",
	"\x1B[7m\\xFF\x1B[27m",
};
#define MAX_ESCAPE_LEN sizeof("\x1B[7m\\xFF\x1B[27m")
#define BUFLEN (4096*4)

int main() {
	unsigned char inbuf[BUFLEN], outbuf[BUFLEN * MAX_ESCAPE_LEN], *ibuf, *obuf;
	unsigned char const *repr;
	size_t len;

	while ((len = fread(inbuf, 1, sizeof(inbuf), stdin))) {
		for (ibuf = inbuf, obuf = outbuf; ibuf != inbuf + len; ++ibuf) {
			repr = escapes[*ibuf];
			memcpy(obuf, repr, strlen((const char *) repr));
			obuf += strlen((const char *) repr);
		}
		fwrite(outbuf, 1, obuf - outbuf, stdout);
		// unsigned char *cur = inbuf, *end = inbuf + len;
		// for (size_t i = 0; i < len; ++cur, ++i) {
		// 	fwrite(escapes[*cur], 1, sizeof(escapes[*cur]), stdout);
		// }
	}
}

